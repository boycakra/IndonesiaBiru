<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.HangoutIn" >
<uses-permission android:name="android.permission.INTERNET">

</uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_indonesiabiru"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_indonesiabiru_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.KpApp">
        <activity android:name="com.example.HangoutIn.MainActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.HangoutIn.Menu"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="com.example.HangoutIn.DivesiteActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="com.example.HangoutIn.About"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="com.example.HangoutIn.MapsActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="com.example.HangoutIn.Eachdivesite"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.example.HangoutIn.Divesiteinfo"
    android:screenOrientation="portrait">
        </activity>
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />
    </application>

</manifest>